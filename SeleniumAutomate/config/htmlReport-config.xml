<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>

		<!-- report theme -->
		<!-- STANDARD, DARK -->
		<theme>STANDARD</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<!-- HTTP, HTTPS -->
		<protocol>HTTPS</protocol>

		<!-- offline report -->
		<timelineEnabled>true</timelineEnabled>

		<!-- viewstyle -->
		<!-- alt-view -->
		<viewstyle></viewstyle>

		<!-- enable or disable timeline on dashboard -->
		<enableTimeline>enable</enableTimeline>

		<!-- title of the document -->
		<documentTitle>SCC-Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>SCC-AutomateTest-Report</reportName>

		<!-- timestamp format -->
		<!-- dd-MMM-yyyy 01-Jan-2022 -->
		<!-- dd-MM-yyyy 01-01-2022 -->
		<timeStampFormat>dd-MMM-yyyy HH:mm:ss</timeStampFormat>

		<argLine>-Dfile.encoding=UTF-8</argLine>

		<!-- custom javascript -->
		<scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
                
            ]]>
		</styles>
	</configuration>
</extentreports>